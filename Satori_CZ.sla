        <PAGEOBJECT XPOS="734.978723404255" YPOS="4591.93617021277" OwnPage="10" ItemID="435261536" PTYPE="2" WIDTH="258.212765957447" HEIGHT="298.446808510638" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.259771394323387" LOCALSCY="0.259771394323387" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="PG_11_02.png" IRENDER="0" EMBEDDED="0" path="M0 0 L258.213 0 L258.213 298.447 L0 298.447 L0 0 Z" copath="M0 0 L258.213 0 L258.213 298.447 L0 298.447 L0 0 Z" gXpos="734.978723404255" gYpos="4591.93617021277" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="736" YPOS="4896.77265354331" OwnPage="10" ItemID="435312512" PTYPE="4" WIDTH="250.553191489362" HEIGHT="78.8869209247732" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L250.553 0 L250.553 78.8869 L0 78.8869 L0 0 Z" copath="M0 0 L250.553 0 L250.553 78.8869 L0 78.8869 L0 0 Z" gXpos="736" gYpos="4014.88288976378" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Příklad:"/>
                <para PARENT="Příklad"/>
                <ITEXT CH="Lara se rozhodne nárokovat oběti. Může si vzít smaragd, ale v tomto případě, protože se jí to hodí, se rozhodne vzít dvě mince. Následně vrátí svou značku na stupnici oběti na první místo (které má ikonu)."/>
                <trail PARENT="Příklad"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="737.127659574468" YPOS="4978.77365354331" OwnPage="10" ItemID="435315344" PTYPE="4" WIDTH="249.425531914894" HEIGHT="96.5625166694554" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L249.426 0 L249.426 96.5625 L0 96.5625 L0 0 Z" copath="M0 0 L249.426 0 L249.426 96.5625 L0 96.5625 L0 0 Z" gXpos="737.127659574468" gYpos="4096.88388976378" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Na třetím poli stupnice obětí můžete vybírat mezi dvěma odměnami: buď si vezmete dvě mince nebo odemknete svůj šestý Jokoro. Pokud si vyberete druhou možnost, vezměte Jokoro vaší barvy z  třetího pole stupnice obětí na herní desce a umístěte ho do své herní oblasti. Nyní máte tento šestý Jokoro k dispozici."/>
                <trail PARENT="Běžný odstavec"/>
            </StoryText>
        </PAGEOBJECT>
