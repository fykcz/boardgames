                <trail PARENT="Příklad"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="997.361702127659" YPOS="1995.61702127659" OwnPage="4" ItemID="327843136" PTYPE="4" WIDTH="252.255319148936" HEIGHT="156.667443290337" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L252.255 0 L252.255 156.667 L0 156.667 L0 0 Z" copath="M0 0 L252.255 0 L252.255 156.667 L0 156.667 L0 0 Z" gXpos="997.361702127659" gYpos="1995.61702127659" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Pokud se barva věřícího shoduje s barvou oltáře, "/>
                <ITEXT FONT="Calibri Bold" CH="využijte co nejvíce akcí na obou stranách nebo na libovolné straně oltáře v libovolném pořad"/>
                <ITEXT CH="í. Pokud je to možné, posuňte značku na stupnici obětí tohoto hráče o dvě políčka vpřed (viz stupnice obětí)."/>
                <para PARENT="Běžný odstavec"/>
                <para/>
                <ITEXT CH="Příklad 3"/>
                <para PARENT="Příklad"/>
                <ITEXT CH="Gael umístí šedého věřícího na šedý oltář. Může využít akce na obou stranách oltáře v libovolném pořadí."/>
