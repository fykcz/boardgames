        <PAGEOBJECT XPOS="216.725409448819" YPOS="3965.12765957447" OwnPage="7" ItemID="335861376" PTYPE="4" WIDTH="437.457530574635" HEIGHT="72.3191489361698" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L437.458 0 L437.458 72.3191 L0 72.3191 L0 0 Z" copath="M0 0 L437.458 0 L437.458 72.3191 L0 72.3191 L0 0 Z" gXpos="216.725409448819" gYpos="3965.12765957447" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Stavba na pagodě"/>
                <para PARENT="Heading 3"/>
                <ITEXT CH="Můžete přispět k postavení pagody tím, že postavíte úroveň nebo střechu. V rámci jedné akce můžete postavit pouze jednu položku. Není dovoleno stavět nad úrovní, kde se nachází Architekt, pouze na jeho úrovni nebo níže."/>
                <trail PARENT="Běžný odstavec"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="140.140386831965" YPOS="4042.89361702128" OwnPage="7" ItemID="335909520" PTYPE="4" WIDTH="514.042553191489" HEIGHT="196.765957446808" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L514.043 0 L514.043 196.766 L0 196.766 L0 0 Z" copath="M0 0 L514.043 0 L514.043 196.766 L0 196.766 L0 0 Z" gXpos="140.140386831965" gYpos="4042.89361702128" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Stavba úrovně"/>
                <para PARENT="Heading 4"/>
                <ITEXT CH="Zaplaťte uvedenou cenu na žeton úrovně pagody a získejte odpovídající body Satori. Poté otočte žeton úrovně pagody, vezměte žeton nákladů na Jokora z balíčku a umístěte jej na postavenou úroveň. Každý z těchto žetonů má pro hry pro 3 a 4 hráče dvě místa pro stavbu Jokorů. V hrách pro 2 hráče nebo ve variantě pro jednoho hráče je k dispozici pouze jedno místo pro Jokory na těchto žetonech."/>
                <para PARENT="Běžný odstavec"/>
                <ITEXT CH="Pokud je vytažen žeton zrušených nákladů Jokoro (což je možné pouze ve hrách pro 3 a 4 hráče), vezměte nový žeton nákladů Jokoro a umístěte na jedno z dostupných míst na novém žetonu."/>
                <para PARENT="Běžný odstavec"/>
                <ITEXT CH="Pokud je při odhalení nového žetonu nákladů Jokoro odhalen další žeton zrušených nákladu Jokora, umístěte jej na úroveň se dvěma žetony neutrálních Jokoro."/>
                <para PARENT="Běžný odstavec"/>
                <ITEXT CH="Místa s žetony neutrálních Jokoro nejsou k dispozici pro hráče k umístění svých Jokorů."/>
                <trail PARENT="Běžný odstavec"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="735.021276595744" YPOS="3587.65957446808" OwnPage="8" ItemID="335912352" PTYPE="2" WIDTH="516.382978723404" HEIGHT="214.212765957447" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="0.134334801957181" LOCALSCY="0.134334801957181" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="0" RATIO="1" Pagenumber="0" PFILE="PG_09_01.png" IRENDER="0" EMBEDDED="0" path="M0 0 L516.383 0 L516.383 214.213 L0 214.213 L0 0 Z" copath="M0 0 L516.383 0 L516.383 214.213 L0 214.213 L0 0 Z" gXpos="735.021276595744" gYpos="3587.65957446808" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1"/>
        <PAGEOBJECT XPOS="735.31914893617" YPOS="3805.51281102362" OwnPage="8" ItemID="335886864" PTYPE="4" WIDTH="514.042553191489" HEIGHT="61.721231529566" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L514.043 0 L514.043 61.7212 L0 61.7212 L0 0 Z" copath="M0 0 L514.043 0 L514.043 61.7212 L0 61.7212 L0 0 Z" gXpos="735.31914893617" gYpos="3805.51281102362" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Příklad:"/>
                <para PARENT="Příklad"/>
                <ITEXT CH="(1) Lara postaví první úroveň pagody, zaplatí 1 smaragd, 1 kámen a 1 dřevo a získá 5 bodů Satori. Poté otočí žeton úrovně pagody a vezme z balíčku žeton nákladů Jokoro. (2) Pokud je to žeton zrušených nákladů Jokoro, vezme nový žeton (3) a umístí jej na úroveň pagody. Umístí žeton neutrálního Jokora na jedno z dostupných polí."/>
                <trail PARENT="Příklad"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="736" YPOS="3887.71753543307" OwnPage="8" ItemID="335915184" PTYPE="4" WIDTH="250.553191489362" HEIGHT="196.329363209921" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L250.553 0 L250.553 196.329 L0 196.329 L0 0 Z" copath="M0 0 L250.553 0 L250.553 196.329 L0 196.329 L0 0 Z" gXpos="736" gYpos="3887.71753543307" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Stavba střechy"/>
                <para PARENT="Heading 4"/>
                <ITEXT CH="Zaplaťte náklady uvedené na poli pro střechu pagody vytištěném na desce a získejte odpovídající body Satori. Nyní umístěte jeden ze svých žetonů meditace z vaší osobní rezervy na pole pro střechu pagody. Nemůžete postavit střechu, pokud nemáte žádný žeton meditace ve své rezervě."/>
                <para PARENT="Běžný odstavec"/>
                <ITEXT CH="Příklad:"/>
                <para PARENT="Příklad"/>
                <ITEXT CH="Gael postaví střechu na třetí úrovni pagody, zaplatí 3 mince a získá 3 body Satori. Poté umístí jeden ze svých "/>
                <ITEXT FONT="Calibri Bold Italic" CH="žetonů meditace z jeho osobní rezervy"/>
                <ITEXT CH=" na odpovídající pole pro střechu pagody."/>
                <trail PARENT="Příklad"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="1000" YPOS="3887.71753543307" OwnPage="8" ItemID="335898192" PTYPE="4" WIDTH="250.553191489362" HEIGHT="210.744680851064" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L250.553 0 L250.553 210.745 L0 210.745 L0 0 Z" copath="M0 0 L250.553 0 L250.553 210.745 L0 210.745 L0 0 Z" gXpos="1000" gYpos="3887.71753543307" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Stavba Sōrin"/>
                <para PARENT="Heading 4"/>
                <ITEXT CH="Ve pátém kole je možné postavit "/>
                <ITEXT FONT="Calibri Bold" CH="Sōrin"/>
                <ITEXT CH=". "/>
                <ITEXT FONT="Calibri Bold" CH="Stavební náklady pro Sōrin jsou 1 mince, 1 dřevo, 1 kámen, 1"/>
                <nbspace FONT="Calibri Bold"/>
                <ITEXT FONT="Calibri Bold" CH="kadidlo a musíte zahodit jeden žeton meditace z vaší osobní rezervy"/>
                <ITEXT CH=". Zaplaťte náklady a otočte žeton Sōrin. Poté si vezměte "/>
                <ITEXT FONT="Calibri Bold" CH="žeton bodů Sōrin"/>
                <ITEXT CH=", který se bude počítat do vašich bodů na konci hry. Umístěte žeton nákladů Jokoro nad Sōrin stejným způsobem jako při stavbě úrovně pagody."/>
                <para PARENT="Běžný odstavec"/>
                <ITEXT CH="Příklad:"/>
                <para PARENT="Příklad"/>
                <ITEXT CH="Lara postaví Sōrin, zaplatí jeho náklady a zahodí jeden žeton meditace. Poté vezme žeton bodů Sōrin a umístí jej na svou herní plochu pro závěrečné bodování."/>
                <trail PARENT="Příklad"/>
            </StoryText>
        </PAGEOBJECT>
