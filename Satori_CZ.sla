        <PAGEOBJECT XPOS="736.232194504941" YPOS="1297.02127659574" OwnPage="2" ItemID="332109328" PTYPE="4" WIDTH="512.680851063829" HEIGHT="406.468085106382" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L512.681 0 L512.681 406.468 L0 406.468 L0 0 Z" copath="M0 0 L512.681 0 L512.681 406.468 L0 406.468 L0 0 Z" gXpos="736.232194504941" gYpos="1297.02127659574" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="3. Příprava hry"/>
                <para PARENT="Heading 1"/>
                <ITEXT CH="3.1 Základní"/>
                <para PARENT="Heading 2"/>
                <ITEXT CH="1."/>
                <tab/>
                <ITEXT CH="Umístěte desku s Velkou horou doprostřed stolu (bez květin na horách)."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="2."/>
                <tab/>
                <ITEXT CH="Do pytle vložte 3/4/5 "/>
                <ITEXT FONT="Calibri Bold" CH="figurky věřících"/>
                <ITEXT CH=" každé barvy pro hru s 2/3/4 hráči. Poté náhodně vyberte 3 figurky věřících na každého hráče z pytle a umístěte je na "/>
                <ITEXT FONT="Calibri Bold" CH="Sanmon"/>
                <ITEXT CH=" (vchodové dveře do chrámu)."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="3."/>
                <tab/>
                <ITEXT CH="Umístěte "/>
                <ITEXT FONT="Calibri Bold" CH="Architekta"/>
                <ITEXT CH=" vedle první úrovně pagody."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="4."/>
                <tab/>
                <ITEXT CH="Zamíchejte 6 žetonů úrovní pagody obrázkem dolů a náhodně vyberte 5, umístěte jeden na každé pole pro úroveň pagody. Zbývající žeton vraťte zpět do krabice."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="5."/>
                <tab/>
                <ITEXT CH="Umístěte žeton Sōrin nad pagodu na část, která ještě nebyla postavena. Zamíchejte čtyři žetony bodování Sōrin a náhodně vyberte jeden. Umístěte ho vedle žetonu Sōrin."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="6."/>
                <tab/>
                <ITEXT CH="Vezměte "/>
                <ITEXT FONT="Calibri Bold" CH="žetony nákladů Jokoro"/>
                <ITEXT CH=" a přidejte 0/3/1 "/>
                <ITEXT FONT="Calibri Bold" CH="žeton(ů/y) zrušených nákladů Jokoro"/>
                <ITEXT CH=" pro hru s 2/3/4 hráči. Zamíchejte je dohromady a položte je obrázkem dolů jako dobírací balíček na jednu stranu desky. Nepoužité "/>
                <ITEXT FONT="Calibri Bold" CH="žetony zrušených nákladů Jokoro"/>
                <ITEXT CH=" vraťte zpět do krabice. Umístěte 0/3/1 neutrální žetony Jokoro vedle desky pro hru s 2/3/4 hráči."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="7."/>
                <tab/>
                <ITEXT CH="Zamíchejte "/>
                <ITEXT FONT="Calibri Bold" CH="žetony oltářů"/>
                <ITEXT CH=" a položte je obrázkem dolů do hromady na straně desky. Náhodně vyberte 3 žetony a umístěte je na 3 místa pro stavbu oltářů."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="8."/>
                <tab/>
                <ITEXT CH="Zamíchejte žetony dekorace oltáře a položte je obrázkem dolů do hromady na straně desky. Umístěte tři žetony, vybrané náhodně, na pole dekorací. Pokud jsou všechny tři identické, vraťte je zpět do hromady, zamíchejte je a znovu vyberte tři nové."/>
                <trail PARENT="Číslovaný seznam"/>
            </StoryText>
        </PAGEOBJECT>
