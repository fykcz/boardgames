            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="9."/>
                <tab/>
                <ITEXT CH="Umístěte zdroje do dosahu všech hráčů (mince, kámen, dřevo a smaragdy)."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="10."/>
                <tab/>
                <ITEXT CH="Rozdělte "/>
                <ITEXT FONT="Calibri Bold" CH="žetony meditace"/>
                <ITEXT CH=" podle éry a zamíchejte každou hromadu, abyste vytvořili dva samostatné dobírací balíčky na straně desky. Vyberte 12 žetonů z hromady první éry a umístěte je na 12 prostranství hor."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="11."/>
                <tab/>
                <ITEXT CH="Umístěte "/>
                <ITEXT FONT="Calibri Bold" CH="žetony Hikari"/>
                <ITEXT CH=", jeden na každý z nejvyšších polí (ze čtyř dostupných polí) na každé hoře."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="12."/>
                <tab/>
                <ITEXT CH="Zamíchejte "/>
                <ITEXT FONT="Calibri Bold" CH="počáteční žetony"/>
                <ITEXT CH=" a vyberte stejný počet žetonů, kolik je hráčů, plus jeden další žeton, umístěte je obrázkem nahoru na stůl. Zbývající vraťte zpět do krabice."/>
                <para PARENT="Číslovaný seznam"/>
                <para PARENT="Heading 2"/>
                <ITEXT CH="3.2 Příprava hráče"/>
                <para PARENT="Heading 2"/>
                <ITEXT CH="13."/>
                <tab/>
                <ITEXT CH="Jeden "/>
                <ITEXT FONT="Calibri Bold" CH="žeton amuletu"/>
                <ITEXT CH=", vybraný náhodně, který není odhalen ostatním hráčům. Ostatní žetony amuletů vraťte zpět do krabice."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="14."/>
                <tab/>
                <ITEXT CH="Každý hráč dostane tři "/>
                <ITEXT FONT="Calibri Bold" CH="mnichy"/>
                <ITEXT CH=", pět "/>
                <ITEXT FONT="Calibri Bold" CH="kadidelnic Jokoro"/>
                <ITEXT CH=" a dva "/>
                <ITEXT FONT="Calibri Bold" CH="zvony"/>
                <ITEXT CH=" odpovídající barvě hráče, které umístí na stůl před sebe jako svou osobní rezervu. Každý hráč umístí svou šestou kadidelnici Jokoro na odpovídajícího pole pro kadidelnice Jokoro na "/>
                <ITEXT FONT="Calibri Bold" CH="stupnici obětí"/>
                <ITEXT CH=" na desce."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="15."/>
                <tab/>
                <ITEXT CH="Tři žetony sekty umístěte do oblasti staveniště na desce."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="16."/>
                <tab/>
                <ITEXT CH="Jeden "/>
                <ITEXT FONT="Calibri Bold" CH="ukazatel bodů Satori"/>
                <ITEXT CH=" umístěte na pozici 5 na bodovací stupnici."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="17."/>
                <tab/>
                <ITEXT CH="Jeden ukazatel "/>
                <ITEXT FONT="Calibri Bold" CH="ruky"/>
                <ITEXT CH=" umístěte na střechu "/>
                <ITEXT FONT="Calibri Bold" CH="modlitebních mlýnků"/>
                <ITEXT CH="."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="18."/>
                <tab/>
                <ITEXT CH="Jeden "/>
                <ITEXT FONT="Calibri Bold" CH="ukazatel stupnice obětí"/>
                <ITEXT CH=" umístěte na první pole "/>
                <ITEXT FONT="Calibri Bold" CH="stupnice obětí"/>
                <ITEXT CH=" (pole u základny stupnice s ikonou)."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="19."/>
                <tab/>
                <ITEXT CH="Jeden ukazatel stupnice kadidla umístěte do pole 0 stupnice kadidla (u základny Velkého Jokora)."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="20."/>
                <tab/>
                <ITEXT CH="Náhodně zvolte začínajícího hráče a umístěte jeden z jeho zvonů na první pole "/>
                <ITEXT FONT="Calibri Bold" CH="stupnice pořadí tahů"/>
                <ITEXT CH=" na levé straně. Poté ostatní hráči umístí své zvony na stejnou stupnici."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="21."/>
                <tab/>
                <ITEXT CH="Začínající hráč a další hráči postupují ve stanoveném pořadí, každý hráč vybere jeden "/>
                <ITEXT FONT="Calibri Bold" CH="počáteční žeton a obdrží uvedené zdroje a bonusy"/>
                <ITEXT CH=". "/>
                <ITEXT FONT="Calibri Bold" CH="Poté vrátí počáteční žetony zpět do krabice"/>
                <ITEXT CH=". Následně "/>
                <ITEXT FONT="Calibri Bold" CH="proti směru"/>
                <ITEXT CH=" stanoveného pořadí, každý hráč umístí jednoho "/>
                <ITEXT FONT="Calibri Bold" CH="mnicha na jakékoli dostupné místo na kterékoli z hor"/>
                <ITEXT CH=". Nepoužitý počáteční žeton se vrátí zpět do krabice. Tímto způsobem začínající hráč jako první vybere počáteční žeton a jako poslední umístí mnicha."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="22."/>
                <tab/>
                <ITEXT CH="Umístěte žetony &quot;+40 SP&quot; vedle desky."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="Příklad 1"/>
                <para PARENT="Příklad"/>
                <ITEXT CH="Lara (modrá) vybere počáteční žeton s obrázkem 1 dřeva, 1 kamene a 1 kadidla. Vezme si tyto první dva zdroje z obecné rezervy a přesune ukazatel na stupnici kadidla na pole 1."/>
                <trail PARENT="Příklad"/>
            </StoryText>
        </PAGEOBJECT>
