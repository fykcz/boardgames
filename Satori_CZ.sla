            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Stupnice obětí"/>
                <para PARENT="Heading 3"/>
                <ITEXT CH="Posuňte svůj ukazatel na stupnici obětí, když jakýkoli hráč, včetně vás samotného, umístí věřícího na kterýkoli z vašich oltářů obsazených kadidelnicí Jokoro:"/>
                <para PARENT="Běžný odstavec"/>
                <ITEXT CH="1."/>
                <tab/>
                <ITEXT CH="Pokud je barva věřícího "/>
                <ITEXT FONT="Calibri Bold" CH="odlišná"/>
                <ITEXT CH=" od barvy oltáře, posuňte se o "/>
                <ITEXT FONT="Calibri Bold" CH="jedno pole"/>
                <ITEXT CH=" na stupnici obětí."/>
                <para PARENT="Číslovaný seznam"/>
                <ITEXT CH="2."/>
                <tab/>
                <ITEXT CH="Pokud je barva věřícího "/>
                <ITEXT FONT="Calibri Bold" CH="stejná"/>
                <ITEXT CH=" jako barva oltáře, posuňte se o "/>
                <ITEXT FONT="Calibri Bold" CH="dvě pole"/>
                <ITEXT CH=" na stupnici obětí."/>
                <para PARENT="Číslovaný seznam"/>
                <para/>
                <ITEXT CH="Příklad 4"/>
                <para PARENT="Příklad"/>
                <ITEXT CH="(1) Gael umístí věřícího stejné barvy na jeden z oltářů Lary (modrá). Gael využije akce na obou stranách oltáře a Lara (modrá) postoupí o dvě pole na stupnici obětí (2)."/>
                <trail PARENT="Příklad"/>
            </StoryText>
        </PAGEOBJECT>
